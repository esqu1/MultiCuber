<center><h1>{{name}}</h1></center>
<ul id="chat"></ul>
<form class="form-inline" id="chatform">
	<div class="form-group">
		<input type="hidden" id="room" value="{{id}}"></input>
	</div>
	<div class="form-group">
		<input class="form-control" type="text" id="msg" autocomplete="off" placeholder="Write your message here...">
	</div>
	<button class="btn btn-default">Send</button>
</form>